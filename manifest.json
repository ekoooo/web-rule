{
  "name": "web ruler",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "web ruler",
  "browser_action": {
    "default_icon": "images/icon_19.png",
    "default_title": "web ruler",
    "default_popup": "pop/popup.html"
  },
  "icons": {
    "128": "images/icon_128.png"
  },
  "permissions": ["tabs", "*://*/*", "file:///*"],
  "background": {
    "persistent": true,
    "scripts": ["js/function.js", "js/background.js"]
  }
}